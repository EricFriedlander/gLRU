avg_chunk = [10 , 50, 100];
cache_prop = [.01, .02, .025, .05, .1, .2, .25, .5];
aPT = .001;
stupd = 1;
avg_len = 30*60;
stepNum = 1000;
kmin = .5;
c_start = 15;
load('prodSimResults.mat')




metr = ['pc', 'pm', 'Tw', 'Td', 'pd']
for i = 1:5
    disp(metr(i))
    temp = results(1,:,:,i) - results(2,:,:,i);
    disp('gLRU larger than segLRU')
    sum(temp(:) > 0)
    disp('gLRU less than segLRU')
    sum(temp(:) < 0)
    disp('gLRU equal to segLRU')
    sum(temp(:) == 0)
    non_zero = results(2,:,:,i) ~= 0;
    disp('Number of segLRU equal to zero')
    sum(non_zero(:))
    disp('Number of where segLRU is zero by gLRU is not')
    sum(temp(:) == 0 & ~non_zero(:))
    rel = temp./results(2,:,:,i);
    disp('Minimum relative difference between gLRU and segLRU')
    min(rel(non_zero))
    median(rel(non_zero))
    max(rel(non_zero))
    pause;
end


for i = 1:5
    i
    temp = results(1,:,:,i) - results(3,:,:,i);
    sum(temp(:) > 0)
    sum(temp(:) < 0)
    sum(temp(:) == 0)
    non_zero = results(3,:,:,i) ~= 0;
    sum(non_zero(:))
    sum(temp(:) == 0 & ~non_zero(:))
    rel = temp./results(3,:,:,i);
    min(rel(non_zero))
    median(rel(non_zero))
    max(rel(non_zero))
    pause;
end